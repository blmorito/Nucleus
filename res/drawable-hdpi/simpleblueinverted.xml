<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
     <item android:state_enabled="false">
	    <shape>
	      <solid
	          android:color="#cacfd2" />
	    
	      <corners
	          android:radius="10dp" />
	      
	      <stroke android:width="2dp"
	          android:color="#0000"/>
	      
	    </shape>
	 </item>
     <item android:state_pressed="true">
	    <shape>
	      <solid
	          android:color="#e8e9fc" />
	     
	      <corners
	          android:radius="10dp" />
	      
	    </shape>
	 </item>
	 
    <item>
	    <shape>
	      <solid
	          android:color="#fff" />
	    
	      <corners
	          android:radius="10dp" />
	      
	      <stroke android:width="2dp"
	          android:color="#00438e"/>
	      
	    </shape>
	 </item>
	 
   
	 
   

</selector>
